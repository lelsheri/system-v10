 "html": "<style>\n\t.print-format table, .print-format tr, \n\t.print-format td, .print-format div, .print-format p {\n\t\tfont-family: Monospace;\n\t\tline-height: 100%;\n\t\tvertical-align: middle;\n\t}\n\t@media screen {\n\t\t.print-format {\n\t\t\twidth: 4in;\n\t\t\tpadding: 0.01in;\n\t\t\tmin-height: 4in;\n\t\t}\n\t}\n</style>\n\n<p class=\"text-center\">\n       \t<b>{{ __(\"Your Name\") }}</b><br>\n\t{{ offline_pos_name }}<b> : {{  __(\"\u0631\u0642\u0645 \u0627\u0644\u0627\u064a\u0635\u0627\u0644\") }}</b><br>\n        {{ user }}<b> : {{ __(\"\u0627\u0644\u0643\u0627\u0634\u064a\u0631\") }}</b><br>\n        {{ customer }}<b> : {{ __(\"\u0627\u0644\u0628\u0627\u0626\u0639\") }}</b>                        \n</p>\n<p class=\"text-right\">\n\t{{ dateutil.global_date_format(posting_date) }}<b> : {{ __(\"\u0627\u0644\u062a\u0627\u0631\u064a\u062e\") }}</b><br>\n        {{ posting_time }}<b> : {{ __(\"\u0627\u0644\u0648\u0642\u062a\") }}</b>\n             \n</p>\n\n\n<table class=\"table table-condensed cart\" border=\"1\">\n\t<thead>\n\t\t<tr>\n                        <th width=\"20%\" class=\"text-center\">{{ __(\"\u0627\u0644\u0627\u062c\u0645\u0627\u0644\u064a\") }}</th>\n\t\t\t<th width=\"20%\" class=\"text-center\">{{ __(\"\u0627\u0644\u0633\u0639\u0631\") }}</th>\n                        <th width=\"20%\" class=\"text-center\">{{ __(\"\u0627\u0644\u0643\u0645\u064a\u0647\") }}</th>\n                        <th width=\"40%\" class=\"text-center\">{{ __(\"Item\") }}</b></th> \n               </tr>\n\t</thead>\n\t<tbody>\n\t\t{% for item in items %}\n\t\t<tr>\n                        <td class=\"text-right\">{{ format_number(item.amount) }}</td>\n                        <td class=\"text-right\">{{ format_number(item.rate) }}</td>\n\t\t\t<td class=\"text-right\">\n                            {{ format_number(item.qty, precision(\"difference\"))}}\n                        </td>\t\t\t\n                        <td class=\"text-center\">\n\t\t            {{ item.item_name }}\n\t\t\t</td>                                              \n\t\t</tr>\n\t\t{% endfor %}\n\t</tbody>\n</table>\n\n<table class=\"table table-condensed\" border=\"1\">\n\t<tbody>\n\t\t<tr>\n\t\t\t\n\t\t\t<td class=\"text-center\" border=\"0.5\">\n\t\t\t\t{{ format_currency(total, currency) }}\n\t\t\t</td>\n                        <td class=\"text-right\" style=\"width: 60%\" border=\"0.5\">\n\t\t\t\t<b>{{ __(\"Net Total\") }}</b>\n\t\t\t</td>\n\t\t</tr>\n\t\t{% for row in taxes %}\n\t\t{% if not row.included_in_print_rate %}\n\t\t<tr>\n\t\t\t<td class=\"text-right\" style=\"width: 60%\" border=\"0.5\">\n\t\t\t\t{{ row.description }}\n\t\t\t</td>\n\t\t\t<td class=\"text-center\" border=\"0.5\">\n\t\t\t\t{{ format_currency(row.tax_amount, currency) }}\n\t\t\t</td>\n\t\t</tr>\n\t\t{% endif %}\n\t\t{% endfor %}\n\t\t{% if discount_amount %}\n\t\t<tr>\n\t\t\t<td class=\"text-right\" style=\"width: 60%\" border=\"0.5\">\n\t\t\t\t{{ __(\"Discount\") }}\n\t\t\t</td>\n\t\t\t<td class=\"text-center\" border=\"0.5\">\n\t\t\t\t{{ format_currency(discount_amount, currency) }}\n\t\t\t</td>\n\t\t</tr>\n\t\t{% endif %}\n\t\t<tr>\n\t\t\t\n\t\t\t<td class=\"text-center\" border=\"0.5\">\n\t\t\t\t{{ format_currency(grand_total, currency) }}\n\t\t\t</td>\n                        <td class=\"text-right\" style=\"width: 60%\" border=\"0.5\">\n\t\t\t\t<b>{{ __(\"Grand Total\") }}</b>\n\t\t\t</td>\n\t\t</tr>\n\t\t<tr>\n\t\t\t\n\t\t\t<td class=\"text-center\" border=\"0.5\">\n\t\t\t\t{{ format_currency(paid_amount, currency) }}\n\t\t\t</td>\n                        <td class=\"text-right\" style=\"width: 60%\" border=\"0.5\">\n\t\t\t\t<b>{{ __(\"\u0627\u0644\u0642\u064a\u0645\u0647 \u0627\u0644\u0645\u062f\u0641\u0648\u0639\u0647\") }}</b>\n\t\t\t</td>\n\t\t</tr>\n                <tr>\n\t\t\t\n\t\t\t<td class=\"text-center\" border=\"0.5\">\n\t\t\t\t{{ format_currency(change_amount, currency) }}\n\t\t\t</td>\n                        <td class=\"text-right\" style=\"width: 60%\" border=\"0.5\">\n\t\t\t\t<b>{{ __(\"\u0627\u0644\u0628\u0627\u0642\u064a\") }}</b>\n\t\t\t</td>\n\t\t</tr>\n\t</tbody>\n</table>\n\n\n\n<p>{{ terms }}</p>\n<p class=\"text-center\">{{ __(\"Bafra \u0634\u0643\u0631\u0627 \u0644\u0632\u064a\u0627\u0631\u062a\u0643\u0645 \u0645\u062d\u0644\u0627\u062a\") }}</p>\n<p class=\"text-center\">{{ __(\"\u0627\u0644\u0627\u0633\u062a\u0628\u062f\u0627\u0644 \u0648\u0627\u0644\u0627\u0633\u062a\u0631\u062c\u0627\u0639 \u0628\u0627\u0644\u0641\u0627\u062a\u0648\u0631\u0647\") }}</p>\n<p class=\"text-center\">{{ __(\"\u0627\u0644\u0627\u0633\u062a\u0628\u062f\u0627\u0644 \u062e\u0644\u0627\u0644 15 \u064a\u0648\u0645\") }}</p>\n<p class=\"text-center\">{{ __(\"\u0644\u0644\u062a\u0648\u0627\u0635\u0644 : 01000000000 - 01000000000\") }}</p>", 
